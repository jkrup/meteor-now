  "description": "Deploy meteor apps with one command using now.sh (https://zeit.co/now)",
  "repository": "https://github.com/jkrup/meteor-now",
  "author": "Justin Krup",
  "contributors": [
    {
      "name": "Mirza Joldic",
      "url": "https://github.com/purplecones"
    }
  ],
  "license": "MIT",
